{
  "metaTitle": "Contact us here at Jaywing",
  "sections": [
    {
      "component": "shared/hero.njk",
      "classList": "o-Container o-Container--centered Text--center",
      "content": {
        "title": "Contact",
        "subTitle": "Why not get in contact? We'd love to hear from you!"
      }
    },
    {
      "component": "shared/form.njk",
      "classList": "o-Container o-Container--thin o-Container--centered",
      "id": "frmContact",
      "action": "#",
      "requiredErrorMessage": "Please enter a valid ",
      "fieldsets": [
        {
          "legend": "Your details",
          "id": "details",
          "fields": [
            {
              "label": "Title",
              "id": "title",
              "type": "select",
              "classList": "",
              "isRequired": true,
              "options": [
                { "title": "" },
                { "title": "Mr", "value": "mr" },
                { "title": "Mrs", "value": "mrs" },
                { "title": "Miss", "value": "miss" },
                { "title": "Ms", "value": "ms" }
              ]
            },
            {
              "label": "First name",
              "id": "fname",
              "type": "text",
              "placeholder": "Joe",
              "isRequired": true,
              "classList": "",
              "maxLength": 30
            },
            {
              "label": "Last name",
              "id": "lname",
              "type": "text",
              "placeholder": "Bloggs",
              "isRequired": true,
              "classList": "",
              "maxLength": 30
            },
            {
              "label": "Email address",
              "id": "email",
              "type": "email",
              "placeholder": "joe@bloggs.com",
              "isRequired": true,
              "classList": "",
              "maxLength": 30
            }
          ]
        }
      ]
    }
  ]
}
