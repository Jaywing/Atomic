{% set pageid = 'index' %}

{% extends '_layouts/application.njk' %}

{% block metaTitle %}{{ metaTitle }}{% endblock %}

{% block content %}

  {% for section in sections %}
    {% include section.component %}
  {% endfor %}

{% endblock %}

{% block javascript %}
  <script src="/javascripts/app.js" async></script>
{% endblock %}
