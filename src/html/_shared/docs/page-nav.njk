<nav id="docs-page-nav" class="docs-Sidenav Sticky-item" data-margin-top="20">

  <ul class="Menu Menu--flush Menu--vertical">

    {% for cat in docs_pages %}

      {% if cat.section == sectionid %}

        <li class="Menu-item">
          {% if cat.category != "" %}<strong class="Menu-title">{{ cat.category }}</strong>{% endif %}
          {% if cat.items %}
            <ul class="Menu Menu--vertical Menu--flush">
              {% for comp in cat.items %}
                {% if comp.section == sectionid %}
                  {{navItem(comp.id, comp.title, pageid, comp.ready)}}
                {% endif %}
              {% endfor %}
            </ul>
          {% endif %}
        </li>

      {% endif %}

    {% endfor %}

  </ul>
</nav>
